IF  EXISTS (SELECT * FROM sys.objects WHERE object_id = OBJECT_ID(N'{databaseOwner}[{objectQualifier}Image]') AND type in (N'U'))
DROP TABLE {databaseOwner}[{objectQualifier}Image]
GO

IF  EXISTS (SELECT * FROM sys.objects WHERE object_id = OBJECT_ID(N'{databaseOwner}[{objectQualifier}Product]') AND type in (N'U'))
DROP TABLE {databaseOwner}[{objectQualifier}Product]
GO
